generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Transport {
  id        Int      @id @default(autoincrement())
  name      String
  takeBy    String
  color     String
  plate     String
  category  String
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt       
}
